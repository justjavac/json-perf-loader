{
  "name": "json-perf-loader",
  "version": "1.0.6",
  "description": "A loader for webpack to load JSON with performance advice",
  "license": "MIT",
  "repository": "justjavac/json-perf-loader",
  "author": "justjavac",
  "homepage": "https://github.com/justjavac/json-perf-loader",
  "bugs": "https://github.com/justjavac/json-perf-loader/issues",
  "main": "src/index.js",
  "engines": {
    "node": "> 8.9.0"
  },
  "scripts": {
    "clean": "rimraf -rf coverage",
    "lint": "prettier \"{**/*,*}.{js,json,md,yml}\" --list-different",
    "format": "prettier \"{**/*,*}.{js,json,md,yml}\" --write",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "cross-env NODE_ENV=test jest --collectCoverageFrom=\"src/**/*.js\" --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "src"
  ],
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "loader-utils": "^2.0.0",
    "schema-utils": "^2.7.1"
  },
  "devDependencies": {
    "codecov": "^3.7.2",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "memory-fs": "^0.5.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.1"
  },
  "keywords": [
    "webpack",
    "webpack-loader",
    "json",
    "perf"
  ]
}
